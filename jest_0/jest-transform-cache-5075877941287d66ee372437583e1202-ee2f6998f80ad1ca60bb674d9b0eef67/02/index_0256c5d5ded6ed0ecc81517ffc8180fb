const {
  INITIAL_BALANCE
} = require("../config");

const {
  ec
} = require("../utils");

class Wallet {
  constructor() {
    this.balance = INITIAL_BALANCE;
    const keyPair = ec.genKeyPair();
    this.publicKey = keyPair.getPublic();
  }

}

module.exports = Wallet;
